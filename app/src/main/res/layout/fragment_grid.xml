<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="bap0031.comp3710.csse.eng.auburn.edu.GridFragment">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:columnCount="4"
            android:rowCount="4">

            <TextView
                android:id="@+id/row0col0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_margin="5dp"
                android:layout_row="0"
                android:text="2" />

            <TextView
                android:id="@+id/row1col0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_margin="5dp"
                android:layout_row="1"
                android:text="2" />

            <TextView
                android:id="@+id/row2col0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_margin="5dp"
                android:layout_row="2"
                android:text="2" />

            <TextView
                android:id="@+id/row3col0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_margin="5dp"
                android:layout_row="3"
                android:text="2" />

            <TextView
                android:id="@+id/row0col1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_margin="5dp"
                android:layout_row="0"
                android:text="2" />

            <TextView
                android:id="@+id/row1col1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_margin="5dp"
                android:layout_row="1"
                android:text="2" />

            <TextView
                android:id="@+id/row2col1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_margin="5dp"
                android:layout_row="2"
                android:text="2" />

            <TextView
                android:id="@+id/row3col1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_margin="5dp"
                android:layout_row="3"
                android:text="2" />

            <TextView
                android:id="@+id/row0col2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_margin="5dp"
                android:layout_row="0"
                android:text="2" />

            <TextView
                android:id="@+id/row1col2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_margin="5dp"
                android:layout_row="1"
                android:text="2" />

            <TextView
                android:id="@+id/row2col2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_margin="5dp"
                android:layout_row="2"
                android:text="2" />

            <TextView
                android:id="@+id/row3col2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_margin="5dp"
                android:layout_row="3"
                android:text="2" />

            <TextView
                android:id="@+id/row0col3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_margin="5dp"
                android:layout_row="0"
                android:text="2" />

            <TextView
                android:id="@+id/row1col3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_margin="5dp"
                android:layout_row="1"
                android:text="2" />

            <TextView
                android:id="@+id/row2col3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_margin="5dp"
                android:layout_row="2"
                android:text="2" />

            <TextView
                android:id="@+id/row3col3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="3"
                android:layout_margin="5dp"
                android:layout_row="3"
                android:text="2" />
        </GridLayout>
    </RelativeLayout>
</FrameLayout>
